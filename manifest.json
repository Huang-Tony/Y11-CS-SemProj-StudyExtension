{
    
    "name": "ProcrastiNOT",

    "description": "A simple tool to stop procrastination",
    "version": "1.0",
    "manifest_version": 3,
    
    "action": {
      "default_popup": "index.html",
      "default_icon": "Extension.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
      ],

  "host_permissions": [
    "<all_urls>",
    "*://*.starwars.com/*",
    "http://*/*",
    "https://*/*"
  ],
   "background":{
    "service_worker": "background.js"
}

  
  }
  
  
  