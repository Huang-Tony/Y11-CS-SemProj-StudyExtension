{
    
    "name": "Study Blocker",

    "description": "A simple tool to stop procrastination",
    "version": "1.0",
    "manifest_version": 3,
    
    "action": {
      "default_popup": "index.html",
      "default_icon": "Extension.png"
    },
    
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "tabs"
      ],

   "background":{
  "scripts": ["background.js"]
}
  }
  
  
  